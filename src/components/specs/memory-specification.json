        "DDR4_2400",
        "DDR4_2666",
        "DDR4_2933",
        "DDR4_3200",
        "DDR4_3600",
        "DDR4_4000",
        "DDR4_4400",
        "DDR5_4800",
        "DDR5_5200",
        "DDR5_5600",
        "DDR5_6000",
        "DDR5_6400",
        "DDR5_7200",
        "DDR5_7800"
